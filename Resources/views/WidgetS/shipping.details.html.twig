{% if data is defined and data is not null %}
    <div class="grid-row m-bottom-40">
        <h5 class="label"><%=translate('salesshipping.shipping.status')%></h5>
        <h4 class="m-top-0 m-bottom-0">
            {{ data.status }}
        </h4>
    </div>

    {% if data.date is defined and data.date is not null %}
        <div class="grid-row m-bottom-40">
            <h5 class="label"><%=translate('salesshipping.shipping.date')%></h5>
            <h4 class="m-top-0 m-bottom-0">
                {{ data.date }}
            </h4>
        </div>
    {% endif %}
{% endif %}
